<!DOCTYPE html>
<html>

<head>
    <!--Import Google Icon Font-->
    <link href="stylesheets/icon.css" rel="stylesheet">
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="stylesheets/materialize.css" media="screen,projection" />
    <!-- Custom css-->
    <link type="text/css" rel="stylesheet" href="stylesheets/custom.css" media="screen,projection" />

    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body>
    <!-- Dropdown Structure -->
    <ul id="dropdown1" class="dropdown-content">
        <li><a href="#!">Profile</a></li>
        <li><a href="#!">Settings</a></li>
        <li class="divider"></li>
        <li><a href="#!">Logout</a></li>
    </ul>
    <nav class="white">
        <div class="container">
            <div class="nav-wrapper">
                <a href="#!" class="brand-logo grey-text">Blobber <span id="brand_badge" class="new badge lime"></span></a>
                <a href="#" data-activates="slide-out" class="button-collapse grey-text"><i class="material-icons">menu</i></a>
                <ul class="right hide-on-med-and-down">
                    <!-- Dropdown Trigger -->
                    <li><a class="dropdown-button grey-text" href="#!" data-activates="dropdown1">Dropdown
                  <i class="material-icons right">arrow_drop_down</i></a></li>
                </ul>
                <ul id="slide-out" class="side-nav">
                    <li>
                        <div class="userView">
                            <div class="background">
                                <img src="images/office.jpg">
                            </div>
                            <a href="#!user"><img class="circle" src="images/yuna.jpg"></a>
                            <a href="#!name"><span class="white-text name">John Doe</span></a>
                            <a href="#!email"><span class="white-text email">jdandturk@gmail.com</span></a>
                        </div>
                    </li>
                    <li><a href="#!"><i class="material-icons">cloud</i>First Link With Icon</a></li>
                    <li><a href="#!">Second Link</a></li>
                    <li>
                        <div class="divider"></div>
                    </li>
                    <li><a class="subheader">Subheader</a></li>
                    <li><a class="waves-effect" href="#!">Third Link With Waves</a></li>
                </ul>
            </div>
        </div>
    </nav>


    <div class="container">
        <!-- Page Content goes here -->

    </div>
    <!--Import jQuery before materialize.js-->
    <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
    <!--Import Materialize.js-->
    <script type="text/javascript" src="javascripts/materialize.js"></script>

    <!--Import Custom.js-->
    <script type="text/javascript" src="javascripts/custom.js"></script>
    <script>
        var message = "<%=message%>";
        if (message) {
            Materialize.toast(message, 4000);
        }
    </script>
</body>

</html>
