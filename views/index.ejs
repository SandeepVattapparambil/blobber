<!DOCTYPE html>
<html>

<head>
	<!--Import Google Icon Font-->
	<link href="stylesheets/icon.css" rel="stylesheet">
	<!--Import materialize.css-->
	<link type="text/css" rel="stylesheet" href="stylesheets/materialize.css" media="screen,projection" />
	<!-- Custom css-->
	<link type="text/css" rel="stylesheet" href="stylesheets/custom.css" media="screen,projection" />

	<!--Let browser know website is optimized for mobile-->
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
</head>

<body class="grey">
	<div class="container">
		<!-- Page Content goes here -->
		<div class="row">
			<div class="col s12 m6 offset-m3 top-margin">
				<div class="card grey darken-1">
					<div class="card-content white-text">
						<span class="card-title">Blobber â€¢ <%=title%></span>
					</div>
					<div class="rule"></div>
					<div class="card-content white-text">
						<div class="row">
              <form id="login_form" name="login-form" method="POST" action="" autocomplete="off">
                <div class="input-field col s12">
  								<input id="username" name="username" type="text" class="validate">
  								<label for="username">Username</label>
  							</div>
  							<div class="input-field col s12">
  								<input id="password" name="password" type="password" class="validate">
  								<label for="password">Password</label>
  							</div>
              </form>
						</div>
					</div>
					<div class="card-action">
						<a href="javascript:void(0)" class="btn-flat lime grey-text waves-effect">Login</a>
					</div>
				</div>

			</div>
		</div>
	</div>


	<!--Import jQuery before materialize.js-->
	<script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
	<!--Import Materialize.js-->
	<script type="text/javascript" src="javascripts/materialize.js"></script>

	<!--Import Custom.js-->
	<script type="text/javascript" src="javascripts/custom.js"></script>
</body>

</html>
